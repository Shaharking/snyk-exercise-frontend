    "babel-preset-stage-2": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.0",
    "eslint-friendly-formatter": "^2.0.6",
